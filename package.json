{
  "scripts": {
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --validate --debug --importMemory --runtime none",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --validate --optimize --importMemory --runtime none",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "server": "http-server . -o -c-1"
  },
  "devDependencies": {
    "http-server": "^0.11.1"
  }
}
